{
  "name": "root",
  "version": "2.1.1",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "clean": "turbo run clean",
    "build": "turbo run build",
    "test": "turbo run test",
    "lint": "eslint .",
    "prettier": "prettier --write \"**/*.{ts,tsx,cts,mts,js,jsx,cjs,mjs}\"",
    "cyclonedx": "cyclonedx-npm --omit dev --omit optional --omit peer"
  },
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "devDependencies": {
    "package-name": "recommended-version",
    ...
  },
  "overrides": {
    "package-name": "recommended-version",
    ...
  },
  "overrides": {
    "esbuild": "0.19.3",
    "got": "11.8.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "trim": "0.0.3"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@medplum/eslint-config"
    ],
    "parserOptions": {
      "project": "./tsconfig.base.json"
    }
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "packageManager": "npm@9.8.1"
}
